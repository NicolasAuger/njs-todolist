extends ../layout

block content
  table(border=1)
    tr
      th Pseudo
      td= user.pseudo
    tr
      th Prénom
      td= user.firstname
    tr
      th Nom
      td= user.lastname
    tr
      th Email
      td= user.email
    tr
      th Crée le :
      td= user.createdAt
    tr
      th Modifié le :
      td= user.modifiedAt
